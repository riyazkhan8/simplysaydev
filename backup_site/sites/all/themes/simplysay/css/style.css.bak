/*  search button css*/
.main-search .input-group-btn > .btn-primary {
  background: rgba(0, 0, 0, 0) none repeat scroll 0 0;
  border: 0 none;
  height: 0;
  margin: 0;
  padding: 0;
  width: 0;
}
.main-search span.icon {
    color: #fff;
    cursor: pointer;
    font-size: 20px;
    margin: auto;
    padding: 10px;
    position: absolute;
    right: -21px;
    top: -19px;
}
.expand li.main-search .search-box {
    margin-right: 40px;
}

.nav .main-search {

    bottom:15px;
}

#social_image_picker #social_images li img {
    margin: 10px 5px;
    max-width: 200px;
}


#social_image_picker ul li {
  display: inline-block;
  margin-bottom: 10px;
  margin-right: 10px;
  width: 31%;
}
#social_image_picker #social_images li img {
  max-width: 100%;
  border: 3px solid transparent;
}
#social_image_picker ul li:nth-child(3n) {
  margin-right: 0;
}
#social_image_picker #social_images li img.selected{
    border-color:#00aff3;
}
.social-media-modal #social_image_picker button {
  background-color: #b4e97a;
  border: 0 none;
  border-radius: 20px;
  color: #2e4c0d;
  display: inline-block;
  font-family: "PantonW01-Bold";
  margin: 0 5px;
  padding: 10px 20px;
}
.social-media-modal .fb-border {
  border: 1px solid #b4e97a;
  padding: 20px;
}
.sample-cards .slick-slide img {
    max-height: 100px;
    max-width: 80px;
    width: 100%;
}
.previewIMG #loading > img {
    max-width: 250px;
}

/****ajax loing popup css*****/
#login-pop{
    padding: 53px 32px 63px;
}
.ajax-register-links > li {
    display: none;
    visibility: hidden;
}
#register-appear {
    display: none;
}
.modal-dialog {
  margin: 10% auto;
}
.register-modal .modal-sm {
  width: 450px;
}
.blankcard-select .form-control {
  background-color: #f5f5f5;
  border: 1px solid #e9e9e9;
  border-radius: 5px;
  color: rgba(0, 0, 0, 0.6);
  font-family: "PantonW01-Bold";
  font-size: 16px;
  height: 52px;
  text-align: center;
}
.btn-primary.form-submit,.form-actions .btn.btn-success.form-submit {
  background: #f8eb00 none repeat scroll 0 0;
  border: 0 none;
  color: #1d1d1d;
  margin-bottom: 20px;
  margin-top: 10px;
  padding: 15px 20px;
  text-transform: uppercase;
}
.control-label[for="edit-remember-me"] {
  font-size: 16px;
  float: left;
}
.login_cnt {
  font-size: 16px;
}
.login_cnt a {
  color: #000;
  font-family: "PantonW01-Bold";
}
.alert ul li {
  float: none;
  margin: 0;
}
.alert ul li a,.help-block.password-help,.register-modal .control-label {
  display: none !important;
}
.form-item.form-item-pass-pass1.form-type-password.form-group.col-sm-6.col-md-4.has-feedback,.form-item.form-item-pass-pass1.form-type-password.form-group.col-sm-6.col-md-4.has-feedback.has-error, .form-item.form-item-pass-pass2.form-type-password.form-group.col-sm-6.col-md-4.has-feedback {
  width: 100%;
}
.has-feedback label ~ .form-control-feedback {
  top: 10px;
}
.nav-tabs > li > a {
  background: transparent;
  color: #000;
  font-size: 16px;
}
.nav-tabs > li > a:hover{
    color: #00AFF3;
    background: transparent;
    }
.nav-tabs > li.active > a {
    color: #00AFF3;
    }
.modal-content .alert .close {
  background: transparent none repeat scroll 0 0;
  height: auto;
  position: initial;
  transform: none;
  width: auto;
}
.form-control {
  height: 40px;
}
.page-header {
  padding-top: 25px;
}
.glyphicon.form-control-feedback.glyphicon-remove {
  top: 25px;
}


