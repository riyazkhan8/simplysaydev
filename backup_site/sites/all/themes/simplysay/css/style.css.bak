/***Front Page Featured Card Css**/


.view-id-featured_card_categories .pager-load-more a {
    background: #f8eb00 none repeat scroll 0 0;
    border: medium none;
    border-radius: 30px;
    color: #2e4c0d;
    display: inline-block;
    font-family: "PantonW01-Bold";
    font-size: 14px;
    margin-top: 10px;
    text-transform: uppercase;
    transition: all 0.2s ease-in-out 0s;
}
.view-id-featured_card_categories .pager-load-more a:hover {
    background: rgba(3, 89, 122, 0.9) none repeat scroll 0 0;
    color: #fff;
    transition: all 0.4s ease-in-out 0s;
}



