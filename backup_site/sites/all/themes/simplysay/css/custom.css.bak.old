/** Slider **/

.ftr_post_slider {
    /*margin: 20px 0px;*/
}

.ftr_post_slider .slick_item {
    margin: 0px;
    overflow: hidden;
}

.ftr_post_slider .slick_item img {
    width: auto;
    /*height: 423px;*/
    max-width: inherit;
    max-height: 18vw;
}

.ftr_post_slider {
    overflow: hidden;
}
#postfooter_slider {
    position: relative;
    margin: 0px -150px;
}
#postfooter_slider .slick-prev:before, 
#postfooter_slider .slick-next:before{
  color:#fff;
}
#postfooter_slider .slick-arrow {
    color: #333;
    cursor: pointer;
    display: block;
    height: 60px;
    position: absolute;
    text-align: center;
    top: 50%;
    left: 18.5%;
    width: 60px;
    z-index: 9;
    font-size: 0px;
    border: none;
    background: none;
    /*margin-top: -30px;*/
    padding: 0px;
    z-index: 9;
    /*text-shadow: 0px 0px 2px #fff;*/
}

#postfooter_slider .slick-arrow.slick-next {
    left: auto;
    right: 17.5%;
}

#postfooter_slider .slick-arrow:before,
#postfooter_slider .slick-arrow:before {
    font-family: "fontawesome";
    font-size: 16px;
    content: "\f053";
    height: 47px;
    width: 47px;
    line-height: 37px;
    text-align: center;
    font-weight: 300;
    opacity: 1;
    background: #00aff3;
    border-radius: 50%;
    border:6px solid #fff;
    display: block;
    padding-right: 3px;
}

#postfooter_slider .slick-arrow.slick-next::before {
    content: "\f054";
    padding-left: 3px;
}

#postfooter_slider::after,
#postfooter_slider::before {
    background: rgba(255, 255, 255, 0.7) none repeat scroll 0 0;
    content: "";
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 19.7%;
    z-index: 4;
}

#postfooter_slider::before {
    left: auto;
    right: 0px;
}
.ftr_post_slider .slick_item {
  position: relative;
}
.ftr_post_slider .slick_item::after, .ftr_post_slider .slick_item::before {
  background: rgba(255, 255, 255, 0.7) none repeat scroll 0 0;
  content: "";
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 100%;
  z-index: 4;
}
.ftr_post_slider .slick_item.slick-current::after, .ftr_post_slider .slick_item.slick-current::before {
  display: none;
}
#postfooter_slider::after, #postfooter_slider::before{display:none;}

